<h1> Books on Books </h1>


<table>
  <tbody>
    <% @books.each do |book| %>
    <tr>
    <td><%= link_to "#{book.title}", book_path(id: book.id)%></td>
    </tr>
    <% end %>
  </tbody>
</table>



<%= form_tag("/books", class: "") do %>
<%= text_field_tag "book[title]" %>
<%= submit_tag %>
<% end %>
